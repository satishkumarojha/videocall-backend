{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "rules": {
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "trace"]
        },
        "max-line-length": [ true, 1000 ], 
        "semicolon": {  
          "options": [
            "always" 
          ]
        }
      },
    "jsRules": {

    }
}